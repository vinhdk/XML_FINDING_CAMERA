<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>MainController</servlet-name>
        <servlet-class>vinhdk.controllers.MainController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CameraController</servlet-name>
        <servlet-class>vinhdk.controllers.CameraController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CardController</servlet-name>
        <servlet-class>vinhdk.controllers.CardController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainController</servlet-name>
        <url-pattern>/MainController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CameraController</servlet-name>
        <url-pattern>/CameraController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CardController</servlet-name>
        <url-pattern>/CardController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>WEB-RENDER/src/index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
