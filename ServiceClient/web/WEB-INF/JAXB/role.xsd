<xsd:schema version="1.0"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns="http://xml.netbeans.org/schema/role"
            targetNamespace="http://xml.netbeans.org/schema/role"
            elementFormDefault="qualified">
    <xsd:element name="roleJAXB">
        <xsd:complexType>
            <xsd:attribute name="id" type="xsd:string" />
            <xsd:attribute name="name" type="xsd:string" />
            <xsd:attribute name="canActiveAccount" type="xsd:boolean" />
            <xsd:attribute name="canActiveRole" type="xsd:boolean" />
            <xsd:attribute name="canActiveBrand" type="xsd:boolean" />
            <xsd:attribute name="canActiveCategory" type="xsd:boolean" />
            <xsd:attribute name="canActiveCapacity" type="xsd:boolean" />
            <xsd:attribute name="canActiveCamera" type="xsd:boolean" />
            <xsd:attribute name="canActiveCard" type="xsd:boolean" />
            <xsd:attribute name="canActiveCrawler" type="xsd:boolean" />
        </xsd:complexType>
    </xsd:element>
</xsd:schema>